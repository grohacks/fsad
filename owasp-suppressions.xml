<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Spring Boot False Positives -->
    <suppress>
        <notes><![CDATA[
        Spring Boot auto-configuration classes are not vulnerable in this context
        ]]></notes>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- MySQL Connector False Positives -->
    <suppress>
        <notes><![CDATA[
        MySQL connector JDBC is not vulnerable in our usage pattern
        ]]></notes>
        <gav regex="true">^mysql:mysql-connector-java:.*$</gav>
        <cve>CVE-2019-2692</cve>
    </suppress>
    
    <!-- Lombok False Positives -->
    <suppress>
        <notes><![CDATA[
        Lombok is a compile-time only dependency
        ]]></notes>
        <gav regex="true">^org\.projectlombok:lombok:.*$</gav>
    </suppress>
    
    <!-- Jackson False Positives for versions we're not using -->
    <suppress>
        <notes><![CDATA[
        False positive for Jackson version we're not using
        ]]></notes>
        <cve>CVE-2020-25649</cve>
    </suppress>
    
    <!-- H2 Database False Positives (Test only) -->
    <suppress>
        <notes><![CDATA[
        H2 database is only used for testing
        ]]></notes>
        <gav regex="true">^com\.h2database:h2:.*$</gav>
    </suppress>
    
</suppressions>